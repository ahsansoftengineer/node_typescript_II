{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "trailing-comma": [ false ],
      "no-console": false,
      "array-type": false,
      "arrow-return-shorthand": true,
      "curly": [true, "ignore-same-line"],
      "deprecation": {
        "severity": "warn"
      }
    },
  "rulesDirectory": []
}